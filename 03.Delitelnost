print("Overenie delitelnosti.")

n = input("Zadaj cislo na delenie: ")
while not n:
    n = input("Zadaj cislo na delenie: ")
k = input("Zadaj delitela: ")

while not k:
    k = input("Zadaj delitela: ")

if k == 0:
    print("Nie je mozne delit nulou.")

v = int(n) % int(k)

if v == 0:
    print(n + " je mozne delit " + k)
else:
    print(n + " nie je delitelne " + k)
